{
	"name": "editor-ui",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build:lib": "vue-tsc --noEmit && vite build --config ./build/lib.config.ts",
		"build:doc": "vue-tsc --noEmit && vite build --config ./build/doc.config.ts && node script/copyDir.js",
		"preview": "vite preview",
		"gen": "node ./script/gen/index.js",
		"lint": "eslint src --ext .vue,.js,.ts,.jsx,.tsx",
		"lint:fix": "eslint src --ext .vue,.js,.ts,.jsx,.tsx --fix",
		"prepare": "husky install"
	},
	"dependencies": {
		"element-plus": "^2.3.8",
		"moment": "^2.29.4",
		"nprogress": "^0.2.0",
		"prism-themes": "^1.9.0",
		"prismjs": "^1.29.0",
		"vue": "^3.3.4"
	},
	"devDependencies": {
		"@babel/core": "^7.22.9",
		"@babel/eslint-parser": "^7.22.9",
		"@commitlint/cli": "^17.6.7",
		"@commitlint/config-conventional": "^17.6.7",
		"@editor-ui/components": "workspace:^",
		"@editor-ui/utils": "workspace:^",
		"@types/node": "^20.4.5",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@vitejs/plugin-vue": "^4.2.3",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-vue": "^9.15.1",
		"fs-extra": "^11.1.1",
		"handlebars": "^4.7.7",
		"husky": "^8.0.0",
		"inquirer": "^9.2.8",
		"prettier": "^3.0.0",
		"sass": "^1.64.1",
		"typescript": "^5.0.2",
		"unplugin-vue-define-options": "^1.3.12",
		"url": "^0.11.1",
		"vite": "^4.4.5",
		"vite-plugin-eslint": "^1.8.1",
		"vite-plugin-md": "^0.22.5",
		"vite-plugin-require-transform": "^1.0.21",
		"vue-router": "^4.0.13",
		"vue-tsc": "^1.8.5"
	}
}
